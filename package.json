{
  "name": "semantic-password-generator",
  "version": "4.0.1",
  "description": "Generate secure password that you can easly read",
  "main": "lib/index.js",
  "author": "Ruben Sospedra",
  "license": "MIT",
  "dependencies": {
    "compromise": "^10.5.0",
    "ent": "^2.2.0",
    "lodash.identity": "^3.0.0",
    "lodash.isnumber": "^3.0.3",
    "lodash.random": "^3.2.0"
  },
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.5.2",
    "nyc": "^11.0.2",
    "standard": "^10.0.2"
  },
  "ava": {
    "source": [
      "src",
      "!demo"
    ],
    "files": [
      "tests",
      "!demo/**/*"
    ]
  },
  "scripts": {
    "build": "babel src -d lib",
    "format": "npm run test:style -- --fix",
